{
  "extends": "gts/tslint.json",
  "rules": {
    "forin": false,
    "variable-name": false,
    "new-parens": false,
    "switch-default": false,
    "no-floating-promises": true,
    "no-any": false,
    "no-conditional-assignment": false,
    "prefer-const": {
      "options": {
        "destructuring": "all"
      }
    }
  }
}
